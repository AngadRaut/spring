<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <!-- bean definitions here -->
    <bean class="org.example.Student" name="student1">
        <property name="stu_id" >
            <value >100</value>
        </property>
        <property name="stu_name" >
            <value >Manoj</value>
        </property>
        <property name="stu_address" >
            <value >Maharashtra</value>
        </property>
    </bean>
    <bean class="org.example.Student" name="student2">
        <property name="stu_id" value="101"/>
        <property name="stu_name" value="Nana"/>
        <property name="stu_address" value="Delhi"/>
    </bean>
<!--    <bean class="org.example.Student" name="student3" p:stu_id="103" p:stu_name="Navin" p:stu_address="Mumbai Maharashtra"/>-->
</beans>
